{
  "tlsKeyFile": "raspberrypi-privkey.pem",
  "tlsCertFile": "raspberrypi-cert.pem",
  "listenAddress": "0.0.0.0",
  "listenPort": 8000,
  "mainPageTitle": "Aaron's Raspberry Pi",
  "templatePageHeaders": {
    "content-type": "text/html",
    "cache-control": "public, max-age=60"
  },
  "commandList": [
    {
      "id": "arp",
      "command": "/usr/sbin/arp -an",
      "description": "arp"
    },
    {
      "id": "df",
      "command": "df -h",
      "description": "df"
    },
    {
      "id": "dmesg",
      "command": "dmesg",
      "description": "dmesg"
    },
    {
      "id": "ifconfig",
      "command": "/sbin/ifconfig",
      "description": "ifconfig"
    },
    {
      "id": "iwconfig",
      "command": "/sbin/iwconfig",
      "description": "iwconfig"
    },
    {
      "id": "log_ls",
      "command": "ls -latrh ~/node-server/logs",
      "description": "log ls"
    },
    {
      "id": "log_tail",
      "command": "tail -n30 ~/node-server/logs/current",
      "description": "log tail"
    },
    {
      "id": "lscpu",
      "command": "lscpu",
      "description": "lscpu"
    },
    {
      "id": "ncal",
      "command": "ncal -h -y",
      "description": "ncal"
    },
    {
      "id": "netstat_an",
      "command": "netstat -an",
      "description": "netstat -an"
    },
    {
      "id": "netstat_s",
      "command": "netstat -s",
      "description": "netstat -s"
    },
    {
      "id": "ntpq",
      "command": "ntpq -p",
      "description": "ntpq"
    },
    {
      "id": "pitemp",
      "command": "/home/pi/bin/pitemp.sh",
      "description": "pitemp"
    },
    {
      "id": "tc_qdisc",
      "command": "/sbin/tc -s qdisc show",
      "description": "tc qdisc show"
    },
    {
      "id": "top",
      "command": "top -b -n1",
      "description": "top"
    },
    {
      "id": "unifi_log",
      "command": "sudo tail -n30 /var/log/unifi/server.log",
      "description": "unifi log"
    },
    {
      "id": "uptime",
      "command": "uptime",
      "description": "uptime"
    },
    {
      "id": "vmstat",
      "command": "vmstat",
      "description": "vmstat"
    },
    {
      "id": "w",
      "command": "w",
      "description": "w"
    }
  ],
  "staticFileList": [
    {
      "httpPath": "/style.css",
      "filePath": "static/style.css",
      "headers": {
        "content-type": "text/css",
        "cache-control": "public, max-age=60"
      },
      "includeInMainPage": false
    },
    {
      "httpPath": "/command.js",
      "filePath": "static/command.js",
      "headers": {
        "content-type": "application/javascript",
        "cache-control": "public, max-age=60"
      },
      "includeInMainPage": false
    },
    {
      "httpPath": "/proxy.js",
      "filePath": "static/proxy.js",
      "headers": {
        "content-type": "application/javascript",
        "cache-control": "public, max-age=60"
      },
      "includeInMainPage": false
    },
    {
      "httpPath": "/favicon.ico",
      "filePath": "static/raspberrypi-favicon.ico",
      "headers": {
        "content-type": "image/x-icon",
        "cache-control": "public, max-age=60"
      },
      "includeInMainPage": false
    }
  ]
}