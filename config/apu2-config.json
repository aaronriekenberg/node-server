{
  "tlsKeyFile": "apu2-privkey.pem",
  "tlsCertFile": "apu2-cert.pem",
  "listenAddress": "192.168.1.1",
  "listenPort": 8000,
  "mainPageTitle": "Aaron's APU2",
  "templatePageHeaders": {
    "content-type": "text/html",
    "cache-control": "public, max-age=60"
  },
  "commandList": [
    {
      "id": "arp",
      "command": "/usr/sbin/arp -an",
      "description": "arp"
    },
    {
      "id": "df",
      "command": "df -h",
      "description": "df"
    },
    {
      "id": "dhcp-lease-list",
      "command": "/home/aaron/dhcp-leases/show-leases.sh",
      "description": "dhcp lease list"
    },
    {
      "id": "dmesg",
      "command": "dmesg",
      "description": "dmesg"
    },
    {
      "id": "ifconfig",
      "command": "/sbin/ifconfig",
      "description": "ifconfig"
    },
    {
      "id": "log_ls",
      "command": "ls -latrh ~/node-server/logs",
      "description": "log ls"
    },
    {
      "id": "log_tail",
      "command": "tail -n30 ~/node-server/logs/current",
      "description": "log tail"
    },
    {
      "id": "netstat_an",
      "command": "netstat -an",
      "description": "netstat -an"
    },
    {
      "id": "netstat_s",
      "command": "netstat -s",
      "description": "netstat -s"
    },
    {
      "id": "ntpctl",
      "command": "ntpctl -sa",
      "description": "ntpctl"
    },
    {
      "id": "sysctl_hw",
      "command": "sysctl hw",
      "description": "sysctl hw"
    },
    {
      "id": "top",
      "command": "top -b",
      "description": "top"
    },
    {
      "id": "uptime",
      "command": "uptime",
      "description": "uptime"
    },
    {
      "id": "vmstat",
      "command": "vmstat",
      "description": "vmstat"
    },
    {
      "id": "w",
      "command": "w",
      "description": "w"
    }
  ],
  "proxyList": [],
  "staticFileList": [
    {
      "httpPath": "/style.css",
      "filePath": "static/style.css",
      "headers": {
        "content-type": "text/css",
        "cache-control": "public, max-age=60"
      },
      "includeInMainPage": false
    },
    {
      "httpPath": "/command.js",
      "filePath": "static/command.js",
      "headers": {
        "content-type": "application/javascript",
        "cache-control": "public, max-age=60"
      },
      "includeInMainPage": false
    },
    {
      "httpPath": "/proxy.js",
      "filePath": "static/proxy.js",
      "headers": {
        "content-type": "application/javascript",
        "cache-control": "public, max-age=60"
      },
      "includeInMainPage": false
    },
    {
      "httpPath": "/favicon.ico",
      "filePath": "static/node-favicon.ico",
      "headers": {
        "content-type": "image/x-icon",
        "cache-control": "public, max-age=60"
      },
      "includeInMainPage": false
    },
    {
      "httpPath": "/pfstat/index.html",
      "filePath": "/var/www/htdocs/pfstat/index.html",
      "headers": {
        "content-type": "text/html",
        "cache-control": "public, max-age=60"
      },
      "includeInMainPage": true
    },
    {
      "httpPath": "/pfstat/pfstat-packets-v4-1day.png",
      "filePath": "/var/www/htdocs/pfstat/pfstat-packets-v4-1day.png",
      "headers": {
        "content-type": "image/png",
        "cache-control": "public, max-age=60"
      },
      "includeInMainPage": false
    },
    {
      "httpPath": "/pfstat/pfstat-packets-v4-1hour.png",
      "filePath": "/var/www/htdocs/pfstat/pfstat-packets-v4-1hour.png",
      "headers": {
        "content-type": "image/png",
        "cache-control": "public, max-age=60"
      },
      "includeInMainPage": false
    },
    {
      "httpPath": "/pfstat/pfstat-packets-v4-7days.png",
      "filePath": "/var/www/htdocs/pfstat/pfstat-packets-v4-7days.png",
      "headers": {
        "content-type": "image/png",
        "cache-control": "public, max-age=60"
      },
      "includeInMainPage": false
    },
    {
      "httpPath": "/pfstat/pfstat-states-1day.png",
      "filePath": "/var/www/htdocs/pfstat/pfstat-states-1day.png",
      "headers": {
        "content-type": "image/png",
        "cache-control": "public, max-age=60"
      },
      "includeInMainPage": false
    },
    {
      "httpPath": "/pfstat/pfstat-v4-1day.png",
      "filePath": "/var/www/htdocs/pfstat/pfstat-v4-1day.png",
      "headers": {
        "content-type": "image/png",
        "cache-control": "public, max-age=60"
      },
      "includeInMainPage": false
    },
    {
      "httpPath": "/pfstat/pfstat-v4-1hour.png",
      "filePath": "/var/www/htdocs/pfstat/pfstat-v4-1hour.png",
      "headers": {
        "content-type": "image/png",
        "cache-control": "public, max-age=60"
      },
      "includeInMainPage": false
    },
    {
      "httpPath": "/pfstat/pfstat-v4-7days.png",
      "filePath": "/var/www/htdocs/pfstat/pfstat-v4-7days.png",
      "headers": {
        "content-type": "image/png",
        "cache-control": "public, max-age=60"
      },
      "includeInMainPage": false
    }
  ]
}
