{
  "tlsKeyFile": "apu2-privkey.pem",
  "tlsCertFile": "apu2-cert.pem",
  "listenAddress": "192.168.1.1",
  "listenPort": 8000,
  "mainPageTitle": "Aaron's APU2",
  "templatePageHeaders": {
    "content-type": "text/html",
    "cache-control": "public, max-age=60"
  },
  "commandList": [
    {
      "id": "arp",
      "command": "/usr/sbin/arp -an",
      "description": "arp"
    },
    {
      "id": "coredns_log_tail",
      "command": "tail -n30 /home/aaron/coredns-run/logs/current",
      "description": "coredns log tail"
    },
    {
      "id": "cpu_vulnerabilities",
      "command": "grep . /sys/devices/system/cpu/vulnerabilities/*",
      "description": "cpu vulnerabilities"
    },
    {
      "id": "df",
      "command": "df -h",
      "description": "df"
    },
    {
      "id": "dhcp-lease-list",
      "command": "/home/aaron/rust-dhcp-leases/target/release/rust-dhcp-leases",
      "description": "dhcp lease list"
    },
    {
      "id": "dmesg",
      "command": "sudo dmesg",
      "description": "dmesg"
    },
    {
      "id": "ifconfig",
      "command": "/sbin/ifconfig",
      "description": "ifconfig"
    },
    {
      "id": "log_ls",
      "command": "ls -latrh ~/node-server/logs",
      "description": "log ls"
    },
    {
      "id": "log_tail",
      "command": "tail -n30 ~/node-server/logs/current",
      "description": "log tail"
    },
    {
      "id": "lscpu",
      "command": "lscpu",
      "description": "lscpu"
    },
    {
      "id": "ncal",
      "command": "ncal -h -y",
      "description": "ncal"
    },
    {
      "id": "netstat_an",
      "command": "netstat -an",
      "description": "netstat -an"
    },
    {
      "id": "netstat_s",
      "command": "netstat -s",
      "description": "netstat -s"
    },
    {
      "id": "ntpq",
      "command": "ntpq -p",
      "description": "ntpq"
    },
    {
      "id": "sensors",
      "command": "sensors",
      "description": "sensors"
    },
    {
      "id": "tc_qdisc",
      "command": "/sbin/tc -s qdisc show",
      "description": "tc qdisc show"
    },
    {
      "id": "top",
      "command": "top -b -n1",
      "description": "top"
    },
    {
      "id": "uptime",
      "command": "uptime",
      "description": "uptime"
    },
    {
      "id": "vmstat",
      "command": "vmstat",
      "description": "vmstat"
    },
    {
      "id": "w",
      "command": "w",
      "description": "w"
    }
  ],
  "proxyList": [
    {
      "id": "coredns_metrics",
      "description": "coredns metrics",
      "options": {
        "hostname": "localhost",
        "port": 9153,
        "path": "/metrics",
        "method": "GET"
      }
    }
  ],
  "staticFileList": [
    {
      "httpPath": "/style.css",
      "filePath": "static/style.css",
      "headers": {
        "content-type": "text/css",
        "cache-control": "public, max-age=60"
      },
      "includeInMainPage": false
    },
    {
      "httpPath": "/command.js",
      "filePath": "static/command.js",
      "headers": {
        "content-type": "application/javascript",
        "cache-control": "public, max-age=60"
      },
      "includeInMainPage": false
    },
    {
      "httpPath": "/proxy.js",
      "filePath": "static/proxy.js",
      "headers": {
        "content-type": "application/javascript",
        "cache-control": "public, max-age=60"
      },
      "includeInMainPage": false
    },
    {
      "httpPath": "/favicon.ico",
      "filePath": "static/debian-favicon.ico",
      "headers": {
        "content-type": "image/x-icon",
        "cache-control": "public, max-age=60"
      },
      "includeInMainPage": false
    },
    {
      "httpPath": "/vnstat/index.html",
      "filePath": "/home/aaron/vnstat/index.html",
      "headers": {
        "content-type": "text/html",
        "cache-control": "public, max-age=60"
