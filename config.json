{
  "tlsKeyFile": "localhost-privkey.pem",
  "tlsCertFile": "localhost-cert.pem",
  "listenAddress": "127.0.0.1",
  "listenPort": "8080",
  "mainPageTitle": "JS Server",
  "commandList": [
  {
    "httpPath": "/ifconfig",
    "command": "/sbin/ifconfig",
    "description": "ifconfig"
  },
  {
    "httpPath": "/sleep",
    "command": "sleep 5",
    "description": "sleep"
  },
  {
    "httpPath": "/uptime",
    "command": "uptime",
    "description": "uptime"
  },
  {
    "httpPath": "/w",
    "command": "w",
    "description": "w"
  }
  ],
  "staticFileList": [
  {
    "httpPath": "/style.css",
    "filePath": "style.css",
    "headers": {
      "content-type": "text/css",
      "cache-control": "max-age=60"
    },
    "includeInMainPage": false
  }
  ]
}
